<div class="m-detail-wrap">
    <div class="m-close"></div>
    <table class="table table-hover table-light">
        <thead>
            <tr class="uppercase">
                <th> 已发总任务数: </th>
                <th> 任务申请中的数量: </th>
                <th> 已发布的任务: </th>
                <th> 已结束的任务: </th>
                <th> 已结束的任务: </th>
                <th> 查看详情</th>
            </tr>
        </thead>
        <tbody>
            {{for data}}
            <tr>
                <td> {{:payTaskNumSum}}</td>
                <td> {{:payTaskNumApproving}} </td>
                <td> {{:payTaskNumDoing}}</td>
                <td> {{:payTaskNumFinished}}</td>
                <td> {{:payTaskOrderNumSum}} </td>
                <td> <a href="javascript:;" target="_blank">点击查看</a> </td>
            </tr>
            {{/for}}
        </tbody>
    </table>
</div>